{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://connect.informatik.uni-ulm.de/coordination/taf-implementation/-/tree/main/TAF_External_Interfaces/Messaging/TAS_TEARDOWN_RESPONSE/",
    "title": "TAS_TEARDOWN_RESPONSE",
    "type": "object",
    "properties": {
        "success": {
          "type": "string"
        },
        "error": {
          "type": "string"
        },
        "attestationCertificate" : {
          "description": "The certificate (*base64 string*) issued by the IAM, attesting to the correct execution of the TAF within an enclave.",
          "type": "string"
        }
      },
      "oneOf": [{
          "required" : [
            "success",
            "attestationCertificate"
          ]},
          {"required" : [
            "error",
            "attestationCertificate"
          ]  
      }]
  }