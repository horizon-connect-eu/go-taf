{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "MBD_NOTIFY",
    "type": "object",
    "properties": {
      "subscriptionId": {
        "type": "string"
      },
      "CPM_REPORT": {
        "type": "object",
        "properties": {
          "generationTime": {
            "type": "number"
          },
          "reporterPseudoId": {
            "type": "number"
          },
          "reporterArteryId": {
            "type": "number"
          },
          "version": {
            "type": "number"
          },
          "observationlocation": {
            "type": "object",
            "properties": {
              "x": {
                "type": "number"
              },
              "y": {
                "type": "number"
              }
            },
            "required": [
              "x",
              "y"
            ]
          },
          "content": {
            "type": "object",
            "properties": {
              "observationSet": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "targetId": {
                      "type": "number"
                    },
                    "check": {
                      "type": "number"
                    }
                  },
                  "required": [
                    "targetId",
                    "check"
                  ]
                }
              },
              "V2XPduEvidence": {
                "type": "object",
                "properties": {
                  "sourceId": {
                    "type": "number"
                  },
                  "referenceTime": {
                    "type": "number"
                  }
                },
                "required": [
                  "sourceId",
                  "referenceTime"
                ]
              }
            },
            "required": [
              "observationSet",
              "V2XPduEvidence"
            ]
          }
        },
        "required": [
          "generationTime",
          "reporterPseudoId",
          "reporterArteryId",
          "version",
          "observationlocation",
          "content"
        ]
      }
    },
    "required": [
      "subscriptionId",
      "CPM_REPORT"
    ]
  }